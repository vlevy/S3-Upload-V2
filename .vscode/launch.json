{
    // VSÂ Code debug configurations for uploader.py
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Real upload to S3",
            "type": "python",
            "request": "launch",
            "module": "uploader",
            "args": [
                "--bucket",
                "file-backups-raichu",
                "--directory",
                "U:/Vic",
                "--storage-class",
                "STANDARD_IA"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Dry-run upload to S3",
            "type": "python",
            "request": "launch",
            "module": "uploader",
            "args": [
                "--bucket",
                "file-backups-raichu",
                "--directory",
                "U:/Vic",
                "--dry-run",
                "--include",
                "Files 1*",
                "--exclude",
                "*FROM AWS",
                "--list-files"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Cleanup multipart uploads",
            "type": "python",
            "request": "launch",
            "module": "uploader",
            "args": [
                "--bucket",
                "file-backups-raichu",
                "--directory",
                "U:/Vic",
                "--cleanup"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}